{
     "version": 2,
     "builds": [
          {
               "src": "api/index.js",
               "use": "@vercel/node"
          },
          {
               "src": "server.js",
               "use": "@vercel/node"
          }
     ],
     "routes": [
          {
               "src": "/api/(.*)",
               "dest": "/api/index.js"
          },
          {
               "src": "/(.*)",
               "dest": "/server.js"
          }
     ]
}